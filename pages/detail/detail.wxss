/* pages/detail/detail.wxss */
@import '../../wxParse/wxParse.wxss';

.play-ad {
  position: absolute;
  left: 50%;
  top: 50%;
  border: solid 1rpx #fff;
  border-radius: 8rpx;
  margin: -30rpx 0 0 -80rpx;
  display: flex;
  align-items: center;
}

.play-video {
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -66rpx 0 0 -66rpx;
  width: 120rpx;
  height: 120rpx;
}

.tab {
  float: left;
  width: 33.333%;
  text-align: center;
  padding: 10rpx 0;
  font-size: 35rpx;
  color: #1b1b1b;
}

.tab_half {
  float: left;
  width: 50%;
  text-align: center;
  padding: 10rpx 0;
  font-size: 35rpx;
  color: #1b1b1b;
}

.topTabSwiper {
  padding-left: 40rpx;
  padding-right: 40rpx;
  background-color: #fff;
  zoom: 1;
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}

.tabBorer {
  border-bottom: 1px solid #ac6601;
  color: #ac6601;
}

.topnav {
  zoom: 1;
  position: fixed;
  top: 0;
  width: 100%;
  background-color: white;
  z-index: 999;
}

.case-page {
  padding: 40rpx;
  height: 999rpx;
}

.list-masonry {
  margin-left: 20px;
  margin-right: 20px;
  column-count: 2;
  column-gap: 20rpx;
  margin-top: 20rpx;
}

.item-masonry {
  background-color: #f3f3f3;
  margin-bottom: 20rpx;
  break-inside: avoid;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.item-masonry image {
  vertical-align: top;
  width: 100%;
  height: 180rpx;
  border-radius: 10rpx;
}

.teacher {
  position: relative;
  display: flex;
  flex-direction: row;
  height: 166rpx;
  width: 100%;
  background-color: #fff8e2;
  padding-left: 40rpx;
  align-items: center;
}

.teacher .info {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  justify-content: center;
}

 .teacher .zhiwei {
  position: absolute;
  top: 125rpx;
  left: 70rpx;
  background: rgba(255, 192, 7, 1);
  border-radius: 4rpx;
  padding: 0 2rpx 0 2rpx;
  font-size: 20rpx;
}


.midlle {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
}

.midlle .cell {
  display: flex;
  flex-direction: row;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.midlle .cell .item {
  display: flex;
  flex-direction: row;
  margin-right: 40rpx;
  align-items: center;
}

.bottom-float {
  position: fixed;
  display: flex;
  bottom: 0;
  background-color: #fefefe;
  width: 750rpx;
  height: 96rpx;
}

.bottom-float .menu {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 33.33333%;
  height: 96rpx;
}

.bottom-float .share {
  width: 44rpx;
  height: 41rpx;
  margin: auto;
  background-image: url('data:image/png;base64,');
  background-size: 44rpx 41rpx;
  background-repeat: no-repeat;
}

.bottom-float .text1 {
  font-size: 24rpx;
  color: #666;
  position: absolute;
  left: 155rpx;
}

.bottom-float .text {
  font-size: 24rpx;
  color: #666;
  margin-left: 10rpx;
}

.bottom-buy {
  position: fixed;
  display: flex;
  align-items: center;
  bottom: 0;
  width: 750rpx;
  height: 96rpx;
  background-color: #fefefe;
}

.modal-content {
  display: flex;
  flex-direction: column;
}

.star-image {
  position: absolute;
  top: 24rpx;
  margin-left: 65rpx;
  margin-top: 70rpx;
  width: 48rpx;
  height: 48rpx;
}

.star-image .item {
  position: absolute;
  top: 24rpx;
  width: 48rpx;
  height: 48rpx;
}

.middle-content {
  display: flex;
  flex-direction: column;
}

.middle-content .list_item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 420rpx;
  font-size: 29rpx;
  color: #444;
  padding: 10rpx 0;
}

.middle-content .item {
  width: 28rpx;
  height: 28rpx;
  border: 2px solid #b0b0b0;
  border-radius: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.rich-img {
  width: 670rpx;
  height: auto;
}

.mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 999999;
}

.modal-content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background-color: #fff;
  border-radius: 6rpx;
}

.confirm-btn {
  width: auto;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  font-size: 29rpx;
  border-radius: 6rpx;
  background-color: #ffbd2d;
  margin-bottom: 20rpx;
}

.catalog {
  display: flex;
  flex-direction: row;
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  border-bottom: 1rpx #efefef solid;
  justify-content: space-between;
  padding-bottom: 20rpx;
}

.catalog_title
{
  margin-left:10rpx;
  font-size:32rpx;
  width:80%;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}

._catalog_title
{
  margin-left:10rpx;
  color: #ac6601;
  font-size:32rpx;
  width:80%;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}

