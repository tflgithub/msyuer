/* pages/detail/detail.wxss */
@import '../../wxParse/wxParse.wxss';

.play-ad {
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 10000;
  border: solid 1rpx #FFFFFF;
  border-radius: 8rpx;
  margin:-30rpx 0 0 -80rpx;
  display: flex;
  align-items: center;
}

.play-video {
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 10000;
  margin:-66rpx 0 0 -66rpx;
  width: 120rpx;
  height: 120rpx;
}

.tab {
  float: left;
  width: 25%;
  text-align: center;
  padding: 10rpx 0;
  font-size: 35rpx;
  color: #1b1b1b;
}

.topTabSwiper {
  margin-left: 40rpx;
  margin-right: 40rpx;
  zoom: 1;
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}

.tabBorer {
  border-bottom: 1px solid #ac6601;
  color: #ac6601;
}

.swiper_con {
  width: 100%;
  height: 1100rpx;
}

.topnav {
  zoom: 1;
  position: fixed;
  top: 0;
  width: 100%;
  background-color: white;
  z-index: 999;
}

.case-page {
  padding: 40rpx;
  height: 999rpx;
}

.list-masonry {
  column-count: 2;
  column-gap: 20rpx;
}

.item-masonry {
  background-color: #f3f3f3;
  margin-bottom: 20rpx;
  break-inside: avoid;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.item-masonry image {
  vertical-align: top;
  width: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.teacher {
  display: flex;
  flex-direction: row;
  width: 650rpx;
  height: 86rpx;
  background-color: #fef7e8;
  border-radius: 43rpx;
  align-items: center;
  padding: 0 0 0 10rpx;
  margin: 40rpx 0 0 40rpx;
}

.midlle {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.midlle .cell {
  display: flex;
  flex-direction: row;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.midlle .cell .item {
  display: flex;
  flex-direction: row;
  margin-right: 40rpx;
  align-items: center;
}

.bottom-float {
  position: fixed;
  display: flex;
  bottom: 0;
  background-color: #fefefe;
  width: 750rpx;
  height: 96rpx;
}

.bottom-float .menu {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 33.33333%;
  height: 96rpx;
}

.bottom-float .share {
  width: 44rpx;
  height: 41rpx;
  margin: auto;
  background-image: url('data:image/png;base64,');
  background-size: 44rpx 41rpx;
  background-repeat: no-repeat;
}

.bottom-float .text1 {
  font-size: 24rpx;
  color: #666;
  position: absolute;
  left: 155rpx;
}

.bottom-float .text {
  font-size: 24rpx;
  color: #666;
  margin-left: 10rpx;
}

.bottom-buy {
  position: fixed;
  display: flex;
  align-items: center;
  bottom: 0;
  width: 750rpx;
  height: 96rpx;
  background-color: #fefefe;
}

.bottom-disabledBuy {
  position: fixed;
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 0;
  width: 750rpx;
  height: 96rpx;
  background-color: #ffbd2d;
  color: #444;
  font-size: 26rpx;
}

.modal-content {
  display: flex;
  flex-direction: column;
}

.star-image {
  position: absolute;
  top: 24rpx;
  margin-left: 65rpx;
  margin-top: 70rpx;
  width: 48rpx;
  height: 48rpx;
}

.star-image .item {
  position: absolute;
  top: 24rpx;
  width: 48rpx;
  height: 48rpx;
}

.middle-content {
  display: flex;
  flex-direction: column;
}

.middle-content .list_item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 420rpx;
  font-size: 29rpx;
  color: #444;
  padding: 10rpx 0;
}

.middle-content .item {
  width: 28rpx;
  height: 28rpx;
  border: 2px solid #b0b0b0;
  border-radius: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.rich-img {
  width: 670rpx;
  height: auto;
}
