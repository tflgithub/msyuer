/* pages/detail/detail.wxss */

.midlle {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.midlle .cell {
  display: flex;
  flex-direction: row;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.midlle .cell .item {
  display: flex;
  flex-direction: row;
  margin-right: 40rpx;
  align-items: center;
}

.time {
  position: absolute;
  bottom: 40rpx;
  left: 200rpx;
  color: #fff;
  font-size: 28rpx;
}

.cell {
  display: flex;
  flex-direction: row;
}

.cell .item {
  display: flex;
  flex-direction: row;
  margin-right: 40rpx;
  align-items: center;
}

.video {
  width: 750rpx;
  height: 750rpx;
}

.video_horizontal {
  width: 100%;
  height: 433rpx;
}

.video_vertical {
  width: 750rpx;
  height: 750rpx;
}

.cover {
  position: absolute;
  top:0;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 463rpx;
  background-color: rgba(0, 0, 0, 0.37);
  justify-content: center;
}

.cover .desc {
  display: flex;
  flex-direction: row;
  justify-content: center;  
}

.cover .desc .first {
  font-size: 26rpx;
  color: #f9d943;
}

.cover .desc .second {
  font-size: 26rpx;
  color: #fff;
}

.cover .btn {
  font-size: 32rpx;
  margin-top: 20rpx;
  width: 154rpx;
  background-color: #f9d943;
}

.play_icon {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin-top: 161rpx;
  margin-left: 325rpx;
  width: 100%;
  height: 422rpx;
}

.topnav {
  position: fixed;
  top: 0;
  z-index: 99;
  background-color: #fff;
  width: 100%;
}

.bottom-float {
  position: fixed;
  display: flex;
  align-items: center;
  bottom: 36rpx;
  left: 153rpx;
  width: 444rpx;
  height: 96rpx;
  /*下面的是关键的设置100%显示*/
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.bottom-float .pf__image {
  text-align: center;
  width: 33.33333%;
  border-right: 1rpx solid #666;
}

.bottom-float .zp__image {
  text-align: center;
  width: 33.33333%;
  border-right: 1rpx solid #666;
}

.modal-content {
  display: flex;
  position: relative;
  flex-direction: column;
  align-items: center;
}

.modal-close {
  position: absolute;
  width: 34rpx;
  height: 34rpx;
  right: 0;
  padding: 20rpx;
}

.star-image {
  position: absolute;
  top: 24rpx;
  margin-left: 120rpx;
  margin-top: 70rpx;
  width: 48rpx;
  height: 48rpx;
}

.star-image .item {
  position: absolute;
  top: 24rpx;
  width: 48rpx;
  height: 48rpx;
}

.middle-content {
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 204rpx;
  margin-left: 40rpx;
  align-items: center;
}

.middle-content .labels {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
}

.label_normal {
  background-color: #e8e8e8;
  width: auto;
  font-size: 34rpx;
  color: #666;
  height: 60rpx;
  border-radius: 50rpx;
  padding: 6rpx 22rpx 0 22rpx;
  margin: 10rpx 10rpx 10rpx 10rpx;
}

.label_checked {
  background-color: #f9d943;
  width: auto;
  font-size: 34rpx;
  color: #000;
  height: 60rpx;
  border-radius: 50rpx;
  padding: 6rpx 22rpx 0 22rpx;
  margin: 10rpx 10rpx 10rpx 10rpx;
}
