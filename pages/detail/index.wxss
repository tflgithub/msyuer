
@import "../../common/templates/worktemplate/workitem.wxss";

.box{
  background-color: #FFFFFF;
  padding:20rpx;
  display: flex;
  flex-direction: column;
  margin: 30rpx 0 30rpx 0;
  padding-left: 30rpx
}

.scroll-header {
  display: flex;
  flex-direction: row;
  white-space: nowrap;
  width: 100%;
  overflow-x:scroll;
  -webkit-overflow-scrolling : touch;
}

.course-image{
  height: 686rpx;
  width: 680rpx;
  border-radius: 9rpx;
  align-items: center;
  margin: 20rpx;
  box-shadow: 0px 0px 30rpx rgba(0, 0, 0, 0.2);
}

.add-button {
  width: 358rpx;
  height: 140rpx;
  position: fixed;
  bottom: 50rpx;
  right: 0;
  left: 0;
  margin: auto;
  background-color: transparent;
  background-size: 358rpx 140rpx;
  background-repeat: no-repeat;
  border: none;
  background-image: url('data:image/png;base64,');
}

.choose-upload-style {
  height: 150rpx;
  width: 100%;
  align-items: center;
  justify-content: center;
  display: flex;
  flex-direction: row;
}

.choose-upload-style image {
  height: 106rpx;
  width: 106rpx;
  margin-right: 30rpx;
}