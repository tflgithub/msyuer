.teacher{
  display: flex;
  padding-bottom: 10rpx;
  flex-direction: row;
  align-items: center;
}

.teacher .text{
  font-size: 24rpx;
  color: #666666;
  text-align: center;
}

.teacher .avatar {
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.cell {
  display: flex;
  flex-direction: row;
}

.cell .item {
  display: flex;
  flex-direction: row;
  margin-right: 40rpx;
  padding-top: 10rpx;
  align-items: center;
}