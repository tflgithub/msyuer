/* pages/home.wxss */
@import "../../common/templates/worktemplate/workitem.wxss";

.swiper-block {
  height: 418rpx;
  width: 100%;
  margin-top: 40rpx
}

.swiper-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 40rpx;
  align-items: flex-start;
  overflow: unset;
}

.slide-view {
  height: 363rpx;
  width:530rpx;
  border-radius: 8rpx;
  box-shadow: 0px 0px 5rpx rgba(0, 0, 0, 0.2);
  z-index: 1;
}

.slide-view image {
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  height: 249rpx;
  width: 530rpx;
}

.active {
  transform: scale(1.14);
  transition: all 0.3s ease-in 0.1s;
  z-index: 20;
}

.swiper-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow: unset;
}

