/* pages/uploadworks/index.wxss */

.course-name {
  margin: 10rpx 36rpx 30rpx 36rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx 0 20rpx 0;
  background-color: white;
  border-radius: 46rpx;
  font-size: 28rpx;
  color: #ffbe2d;
  overflow: hidden;
  text-overflow: ellipsis;
}

.textarea {
  margin: 0 36rpx 0 36rpx;
  padding: 20rpx 14rpx 20rpx 14rpx;
  font-size: 28rpx;
  position: relative;
  width: 650rpx;
  height: 196rpx;
  border: #e8e8e8 solid 1rpx;
  border-radius: 8rpx;
}

.add-image {
  height: 210rpx;
  width: 210rpx;
}

.image {
  height: 210rpx;
  width: 210rpx;
  border-radius: 4%;
}

.upload-image {
  float: left;
  position: relative;
  margin-right: 24rpx;
  margin-bottom: 10rpx;
}

.delete-image {
  position: absolute;
  top: 0;
  right: 0;
  width: 32rpx;
  height: 32rpx;
  padding: 15rpx;
}

.button {
  margin: 10rpx 36rpx 30rpx 36rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
  position: relative;
  color: white;
  background-color: #ffbd2d;
  border-radius: 2px;
}

/*  重写 checkbox 样式  */

/* 未选中的 背景样式 */

checkbox .wx-checkbox-input {
  margin-left: 36rpx;
  border-radius: 50%; /* 圆角 */
  width: 30rpx; /* 背景的宽 */
  height: 30rpx; /* 背景的高 */
}

/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #ffbe2d;
}

/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%; /* 圆角 */
  width: 30rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 30rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx; /* 对勾大小 30rpx */
  color: #fff; /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.user-info {
  display: flex;
  padding: 20rpx 0 0 20rpx;
  flex-direction: row;
  align-items: center;
}

.user-info image {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.user-info text {
  font-size: 26rpx;
  color: #383a3e;
}

.work-info {
  display: flex;
  margin: 20rpx 20rpx 0 20rpx;
  flex-direction: row;
  height: 160rpx;
  border-radius: 8rpx;
  border: solid 1rpx #f5f5f5;
}

.work-info image {
  height: 100%;
  width: 190rpx;
}

.work-desc {
  width: 160rpx;
  display: flex;
  padding: 10rpx;
  flex-direction: column;
}

.modal-course-name {
  color: #707174;
  font-size: 26rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.modal-content {
  color: #a5a5a5;
  font-size: 22rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -moz-box-orient: vertical;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
}

.commit-image {
  height: 168rpx;
  width: 664rpx;
  position: fixed;
  top: 580rpx;
  right: 40rpx;
}

.iphone-x-commit-image {
  height: 168rpx;
  width: 664rpx;
  position: fixed;
  top: 706rpx;
  right: 40rpx;
}

